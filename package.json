{
  "name": "nativescript-drop-down",
  "version": "3.1.3",
  "description": "A NativeScript DropDown widget.",
  "main": "drop-down",
  "typings": "drop-down.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "3.0.0",
      "android": "3.0.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Interface"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/NativeScript-Drop-Down"
  },
  "scripts": {
    "demo-ios": "grunt build && cd demo && tns run ios",
    "demo-android": "grunt build && cd demo && tns run android",
    "demo-ng-ios": "grunt build && cd demo-ng && tns run ios",
    "demo-ng-android": "grunt build && cd demo-ng && tns run android"
  },
  "keywords": [
    "NativeScript",
    "DropDown",
    "Spinner",
    "tangra"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "contributors": [
    "Dick Smith (https://github.com/DickSmith)"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "tns-core-modules": "^3.1.0 || >3.3.0-"
  },
  "devDependencies": {
    "@angular/common": "~4.4.1",
    "@angular/compiler": "~4.4.1",
    "@angular/compiler-cli": "~4.4.1",
    "@angular/core": "~4.4.1",
    "@angular/forms": "~4.4.1",
    "@angular/http": "~4.4.1",
    "@angular/platform-browser": "~4.4.1",
    "@angular/router": "~4.4.1",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-exec": "2.0.0",
    "nativescript-angular": "4.4.0",
    "rxjs": "~5.0.1",
    "tns-core-modules": "^3.0.0",
    "tns-platform-declarations": "3.0.1",
    "tslint": "^4.5.1",
    "typescript": "~2.2.2",
    "zone.js": "~0.8.12"
  }
}
